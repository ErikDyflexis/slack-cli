{
  "output": "slack.phar",
  "chmod": "0755",
  "main": "slack",
  "files": [
    "LICENSE",
    "vendor/herrera-io/phar-update/res/schema.json"
  ],
  "finder": [
    {
      "name": ["*.php", "*.yml"],
